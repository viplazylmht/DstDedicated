        },
        "id": "mF9Ib3h_bV7k",
        "outputId": "b62bbd59-b6fc-4cb9-d2bc-d76155145637"
      },
      "source": [
        "%cd $PR_DIR\n",
        "\n",
        "!git clone https://github.com/viplazylmht/DstDedicated.git\n",
        "\n",
        "!cp DstDedicated/run_dedicated_servers.sh ."
      ],
      "execution_count": 24,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
